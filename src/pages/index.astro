---
import MainLayout from "../layouts/MainLayout.astro"
---
<head>
    <title>Home | IGNITAX</title>
</head>
<MainLayout>
    
	<main class="fix">
		<!-- Hero Section -->
		<section class="hero-wrapper hero-3 overflow-hidden bg-cover" data-background="/img/hero/hero-bg3-1.png">
            <div class="container">
                <div class="row gy-5 align-items-center">
                    <div class="col-lg-6">
                        <div class="hero-style3">
                            <h1 class="hero-title"><span>Simple Tax Solutions</span>, Secure Financial Futures.</h1>
                            <div class="hero-text text-white">Effortlessly navigate your taxes with expert guidance and a clear roadmap to financial success.</div>
                            <div class="tg-button-wrap mt-40">
                                <a href="/contact" class="btn btn-three">
                                    <span class="btn-text" data-text="Make An Appointment"></span>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="hero-thumb3">
                            <div class="img1">
                                <img src="/img/hero/hero-3-1.png" alt="img">
                            </div>
                            <div class="img2">
                                <img src="/img/hero/hero-3-2.png" alt="img">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

		<!-- Counter Section -->
		<section class="counter-area-3 pt-120 pb-120 overflow-hidden">
            <div class="container">
                <div class="row gy-30">
                    <div class="col-xl-3 col-md-6">
                        <div class="counter-card3">
                            <div class="box-content">
                                <h3 class="counter-card_title"><span class="counter-number">20</span>K</h3>
                            </div>
                            <div class="box-thumb">
                                <img src="/img/others/counter3-1.jpg" alt="img">
                                <p class="counter-card_text">Session Completed</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6">
                        <div class="counter-card3">
                            <div class="box-content">
                                <h3 class="counter-card_title"><span class="counter-number">35</span>+</h3>
                            </div>
                            <div class="box-thumb">
                                <img src="/img/others/counter3-2.jpg" alt="img">
                                <p class="counter-card_text">COUNTRY OPERATION</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6">
                        <div class="counter-card3">
                            <div class="box-content">
                                <h3 class="counter-card_title"><span class="counter-number">20</span>K</h3>
                            </div>
                            <div class="box-thumb">
                                <img src="/img/others/counter3-3.jpg" alt="img">
                                <p class="counter-card_text">EXPERT CONSULTANTS</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6">
                        <div class="counter-card3">
                            <div class="box-content">
                                <h3 class="counter-card_title"><span class="counter-number">100</span>%</h3>
                            </div>
                            <div class="box-thumb">
                                <img src="/img/others/counter3-4.jpg" alt="img">
                                <p class="counter-card_text">COUNTRY OPERATION</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

